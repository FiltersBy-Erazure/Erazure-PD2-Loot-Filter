//----------------------------------------------------------------------------------
// ==============================  Arrows and Bolts  ===============================
//----------------------------------------------------------------------------------

ItemDisplay[NMAG aqv]:// Hides nonmagical Arrow quivers
ItemDisplay[NMAG cqv]:// Hides nonmagical Bolt quivers

//----------------------------------------------------------------------------------
// ===================================  Gold  ======================================
//----------------------------------------------------------------------------------

ItemDisplay[GOLD<10]:// Hide 10 gold and less at all times
ItemDisplay[GOLD<25 AND CLVL>4]:// Hide 25 gold and less if level 5+
ItemDisplay[GOLD<50 AND CLVL>7]:// Hide 50 gold and less if level 8+
ItemDisplay[GOLD<100 AND CLVL>10]:// Hide 100 gold and less if level 11+
ItemDisplay[GOLD<200 AND CLVL>14]:// Hide 200 gold and less if level 15+
ItemDisplay[GOLD<300 AND CLVL>21]:// Hide 300 gold and less if level 22+
ItemDisplay[GOLD<500 AND CLVL>28]:// Hide 500 gold and less if level 29+
ItemDisplay[GOLD<899 AND CLVL>35]:// Hide 900 gold and less if level 36+
ItemDisplay[GOLD<1499 AND CLVL>49]:// Hide 1500 gold and less if level 40+
ItemDisplay[GOLD<2499 AND CLVL>59]:// Hide 2500 gold and less if level 60+
ItemDisplay[GOLD<4499 AND CLVL>79]:// Hide 4500 gold and less if level 80+
ItemDisplay[GOLD<6499 AND CLVL>84]:// Hide 6500 gold and less if level 85+
ItemDisplay[GOLD<9999 AND CLVL>89]:// Hide 10000 gold and less if level 90+
ItemDisplay[GOLD<14999 AND CLVL>94]:// Hide 15000 gold and less if level 95+

//------------------------------------------------------------------------------------
// ================================  Health Potions  =================================
//------------------------------------------------------------------------------------

// Hide lower quality potions based on character level
ItemDisplay[hp1 CLVL>10]:{%NL%%RED% |(%WHITE% Minor %RED%)| }//             	                      Hides Minor Heal Potions after level 11
ItemDisplay[hp2 CLVL>22]:{%NL%%RED% |(%WHITE% Light %RED%)| }//                                       Hides Light Heal Potions after level 19
ItemDisplay[hp3 CLVL>26]:{%NL%%RED% i|(%WHITE% Health %RED%)|i }//                                    Hides Healing Potions after level 26

// Greater Healing Potion
ItemDisplay[hp4 CLVL>34]:{%NL%%RED% i|(%WHITE% Greater %RED%)|i }//                                   Hides Greater Healing Potions after level 35

// Super Healing Potion
ItemDisplay[hp5 CLVL>95]:{%NL%%WHITE% Super Healing Potion }//%RED%i|(%WHITE% H %RED%)|i//            Hides Super Health Potions after level 95 (Remove first set of // to show past level 95)
ItemDisplay[hp5 (CLVL<96 AND CLVL>89)]:{%WHITE% Super Healing Potion }%RED%i|(%WHITE% H %RED%)|i//    Display between level 90 and 95
ItemDisplay[hp5 (CLVL<90 AND CLVL>79)]:{%WHITE% Super Healing Potion }%RED%i|(%WHITE% H %RED%)|i//    Display between level 80 and 89
ItemDisplay[hp5 (CLVL<80 AND CLVL>34)]:{%WHITE% Super Healing Potion }%RED%i|(%WHITE% H %RED%)|i//    Display between level 35 and 79

// Display for Healing Potions during levels 1-35
ItemDisplay[hp1 CLVL<11]:{%WHITE% Minor Healing Potion }%RED%|(%WHITE% Minor %RED%)|//
ItemDisplay[hp2 CLVL<23]:{%WHITE% Light Healing Potion }%RED%* |(%WHITE% Light %RED%)| *//
ItemDisplay[hp3 CLVL<27]:{%WHITE% Healing Potion }%RED%* i|(%WHITE% Health %RED%)|i *//
ItemDisplay[hp4 CLVL<35]:{%WHITE% Greater Healing Potion }%RED%* i|(%WHITE% Greater %RED%)|i *//
ItemDisplay[hp5 CLVL<35]:{%WHITE% Super Healing Potion }%RED%o i|(%WHITE% Super %RED%)|i o//

